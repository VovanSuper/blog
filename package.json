	"version": "0.2.1",
	"engines": {
		"node": ">20.10.0"
	},
	"author": {
		"name": "Vladimir Ovsyukov",
		"email": "me@ovsyukov.ru",
		"url": "https://www.ovsyukov.ru"
	},
	"description": "https://www.ovsyukov.ru site source code repository",
	"scripts": {
		"build": "tsc && vite build",
		"commit": "cz",
		"dev": "vite --open",
		"prepare": "husky install",
		"preview": "vite preview http://localhost:4173",
		"preview:test": "run-p start-server-and-test preview ",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:e2e": "run-s preview:test cy:open",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"start-server-and-test": "start-server-and-test",
		"test:e2e:headless": "run-p preview:test cy:run",
		"test:e2e:ci": "vite build && npm preview:test cy:run",
		"format": "prettier -uw --ignore-path .gitignore .",
		"run-tsc": "tsc",
		"run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.tsx src",
		"lint": "run-p run-tsc run-eslint",
		"validate": "run-p lint test:ci test:e2e:headless",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@tanstack/react-query": "5.29.0",
