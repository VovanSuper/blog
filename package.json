		"@marp-team/marp-cli": "^3.4.0",
		"@nabla/vite-plugin-eslint": "^2.0.2",
		"@originjs/vite-plugin-commonjs": "^1.0.3",
		"@storybook/addon-actions": "^8.0.6",
		"@storybook/addon-essentials": "^8.0.6",
		"@storybook/addon-interactions": "^8.0.6",
